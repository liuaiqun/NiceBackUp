<?xml version="1.0" encoding="UTF-8"?>

<openerp>
	<data>
		
		<record id="res_company_cust_form" model="ir.ui.view">
			<field name="name">res.company.cust.form</field>
			<field name="model">res.company</field>
			<field name="inherit_id" ref="base.view_company_form"/>
			<field name="arch" type="xml">
				<xpath expr="//group[@name='account_grp']" position="after">
					<group>
						<group name="is_multi_warehouse" string="Multi Warehouse">
							<field name="is_multi_warehouse"/>
						</group>
					</group>
				</xpath>
			</field>
		</record>

	</data>
</openerp>